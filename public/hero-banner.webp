<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="600" height="400" fill="#f0f9ff"/>
  
  <!-- Sky gradient -->
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87ceeb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e0f6ff;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="houseGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#fbbf24;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f59e0b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Sky -->
  <rect width="600" height="200" fill="url(#skyGradient)"/>
  
  <!-- Ground -->
  <rect y="200" width="600" height="200" fill="#22c55e"/>
  
  <!-- House structure -->
  <!-- Main house body -->
  <rect x="150" y="120" width="300" height="200" fill="#dc2626" stroke="#991b1b" stroke-width="2"/>
  
  <!-- Roof -->
  <polygon points="130,120 300,50 470,120" fill="#7c2d12" stroke="#451a03" stroke-width="2"/>
  
  <!-- Door -->
  <rect x="270" y="220" width="60" height="100" fill="#92400e" stroke="#78350f" stroke-width="2"/>
  <circle cx="315" cy="270" r="3" fill="#fbbf24"/>
  
  <!-- Windows -->
  <rect x="180" y="150" width="50" height="40" fill="#3b82f6" stroke="#1e40af" stroke-width="2"/>
  <rect x="370" y="150" width="50" height="40" fill="#3b82f6" stroke="#1e40af" stroke-width="2"/>
  
  <!-- Window frames -->
  <line x1="205" y1="150" x2="205" y2="190" stroke="#1e40af" stroke-width="1"/>
  <line x1="180" y1="170" x2="230" y2="170" stroke="#1e40af" stroke-width="1"/>
  <line x1="395" y1="150" x2="395" y2="190" stroke="#1e40af" stroke-width="1"/>
  <line x1="370" y1="170" x2="420" y2="170" stroke="#1e40af" stroke-width="1"/>
  
  <!-- Chimney -->
  <rect x="380" y="70" width="30" height="60" fill="#7c2d12" stroke="#451a03" stroke-width="2"/>
  
  <!-- Trees -->
  <!-- Tree 1 -->
  <rect x="80" y="180" width="15" height="40" fill="#92400e"/>
  <circle cx="87.5" cy="180" r="25" fill="#16a34a"/>
  
  <!-- Tree 2 -->
  <rect x="520" y="170" width="20" height="50" fill="#92400e"/>
  <circle cx="530" cy="170" r="30" fill="#16a34a"/>
  
  <!-- Clouds -->
  <circle cx="100" cy="60" r="20" fill="white" opacity="0.8"/>
  <circle cx="120" cy="60" r="25" fill="white" opacity="0.8"/>
  <circle cx="140" cy="60" r="20" fill="white" opacity="0.8"/>
  
  <circle cx="450" cy="40" r="15" fill="white" opacity="0.8"/>
  <circle cx="465" cy="40" r="20" fill="white" opacity="0.8"/>
  <circle cx="480" cy="40" r="15" fill="white" opacity="0.8"/>
  
  <!-- Sun -->
  <circle cx="500" cy="80" r="30" fill="#fbbf24" opacity="0.9"/>
  
  <!-- Fence -->
  <rect x="50" y="250" width="500" height="3" fill="#78350f"/>
  <rect x="60" y="240" width="3" height="20" fill="#78350f"/>
  <rect x="80" y="240" width="3" height="20" fill="#78350f"/>
  <rect x="100" y="240" width="3" height="20" fill="#78350f"/>
  <rect x="120" y="240" width="3" height="20" fill="#78350f"/>
  <rect x="480" y="240" width="3" height="20" fill="#78350f"/>
  <rect x="500" y="240" width="3" height="20" fill="#78350f"/>
  <rect x="520" y="240" width="3" height="20" fill="#78350f"/>
  <rect x="540" y="240" width="3" height="20" fill="#78350f"/>
</svg>